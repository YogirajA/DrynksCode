@model DrynksMe.Portal.Models.VenueModel

@{
    ViewBag.Title = "Add";
}
<div>
    @CustomHtml.VenueNavPill(this)
</div>
<hgroup class="title">
    <h2>Add venues</h2>
</hgroup>
<p>
    This is where you Add Venues to Database
</p>
<div class="form-horizontal well">
     <div class="row">
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)
            <div class="span12">
                @Html.Partial("_VenueFormFields")
                <div class="control-group">
                    <div class="controls">
                        <input class="btn btn-primary" type="submit" value="Add" />
                    </div>
                </div>
            </div>
        }
     </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
